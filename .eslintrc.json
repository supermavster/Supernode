{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb-base", "plugin:shopify/esnext", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "import/no-unused-modules": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "no-unused-vars": 0,
    "no-console": 1,
    "prettier/prettier": 2
  }
}
